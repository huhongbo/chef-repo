{
	"handlers":{
		"default":{
			"type":"set",
			"handlers":["gelf","keepalive","event_file"]
		},
		"event":{
			"type": "pipe",
			"command": "/etc/sensu/handlers/system_event_handler.rb"
		},
		"gelf": {
		     "type": "pipe",
		     "command": "/etc/sensu/handlers/gelf.rb"
		},
		"keepalive": {
		     "type": "pipe",
             "command": "/etc/sensu/handlers/handler_keepalive.rb"
		},
		"event_file": {
			"type": "pipe",
			"command": "/etc/sensu/handlers/event_files.rb"
		},
		"graphite":{
			"type":"amqp",
			"exchange":{
				"type":"topic",
				"name":"graphite",
				"passive":"true"
				},
			"mutator": "only_check_output"
		}
	}
}